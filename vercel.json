{
  "version": 2,
  "builds": [
    {
      "src": "doctor-patient-chat/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "doctor-patient-chat/server/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/doctor-patient-chat/server/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/doctor-patient-chat/client/build/$1"
    }
  ]
}
